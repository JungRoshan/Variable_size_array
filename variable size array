#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int n;
    cout<<"enter size\n";
    cin>>n;
   int A[n]={1,2,3,4,5,6};//A[n] is variable size array//
   for(int x:A)//for each loop//
   {
       
   cout<<x<<endl;
   }
   cout<<sizeof(A)<<endl;//size of A//
   return 0;
}
